{block #title}{$funnel->name} ({$fromLevel} < {$toLevel}){/block}

{block #content}

<div class="row">
    <div class="col-md-12">
        <a n:href=":SalesFunnel:SalesFunnelsAdmin:show $funnel->id"><i class="fa fa-angle-left"></i> {_sales_funnel.admin.distribution.back_to_funnel}</a>

        <div class="page-header">
            <h1>
                {_sales_funnel.admin.sales_funnels.show.distribution.payments}:
                {$fromLevel}{if $toLevel-1 > $fromLevel} - {$toLevel-1}{elseif $toLevel === null}+{/if}
                <small>
                    <br/>({$funnel->name})
                    / {_system.total} {count($distributionList)}
                </small>
            </h1>
        </div>
    </div>
</div>

<div class="row">
    {include userList.latte}

    {control paymentDistribution $funnel->id}
</div>
